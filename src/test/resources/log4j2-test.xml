<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" strict="true">
  <Appenders>
    <Appender type="Console" name="console">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %class{2.}:%line .%method() - %msg%n"/>
    </Appender>
  </Appenders>
  <Loggers>
    <Logger name="org.r0bb3n" level="INFO"/>
    <Root level="INFO">
      <AppenderRef ref="console"/>
    </Root>
  </Loggers>
</Configuration>